{
  "name": "vuejs-parcel",
  "version": "1.0.0",
  "description": "Vuejs started project using parceljs and tailwindcss",
  "main": "index.js",
  "keywords": [
    "tailwindcss",
    "vuejs",
    "parceljs",
    "boilerplate"
  ],
  "author": "Tommy Pratama <tommyhpratama@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "parcel serve src/**/*.html --no-cache --no-source-maps --no-autoinstall",
    "build": "NODE_ENV=production parcel build src/**/*.html --no-cache --no-source-maps --no-autoinstall --public-url './'",
    "build:github": "parcel build src/**/*.html --public-url https://tommypratama.github.io/start-parcel-vue/"
  },
  "dependencies": {
    "tailwindcss": "^1.8.10",
    "vue": "^2.6.12",
    "vue-hot-reload-api": "^2.3.4"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.2.0",
    "autoprefixer": "^9.8.6",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^3.2.2",
    "prettier": "^2.1.2",
    "vue-template-compiler": "^2.6.12"
  }
}
